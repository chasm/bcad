</script>

<script type="text/x-handlebars" data-template-name="registrants/new">
  <p>Add an Registrant</p>
</script>

<script type="text/x-handlebars" data-template-name="registrants/detail">
  <p>Registrant with email {{email}}</p>
</script>

<script type="text/x-handlebars" data-template-name="registrants/edit">
  <p>Edit Registrant with email {{email}}</p>
</script>

<script type="text/x-handlebars" data-template-name="users">
  <div class="row">
    <div class="span3">
      <table class="list-table">
        <caption>Users</caption>
        <tbody>
          {{#each user in model}}
            <tr>
              <td>{{#linkTo users.detail user}}{{user.name}}{{/linkTo}}</td>
